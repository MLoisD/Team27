n    setBookname(e.target.value)\n  }\n  const handleChangeauthor= (e) => {\n    setAuthor(e.target.value)\n  }\n  const handleChangestock= (e) => {\n    setStock(e.target.value)\n  }\n  const handleChangegenre= (e) => {\n    setGenre(e.target.value)\n  }\n\n  const handleUpdate = (e) => {\n     e.preventDefault();\n    const itemObj = {\n      id: valId,\n      bookname: bookname,\n      author: author,\n      stock: stock,\n      genre: genre,\n      img: file\n    }\n    let arr = []\n    arr.push(itemObj);\n    console.log(\"arr\", arr);\n  }\n\n\n      return (\n      <div className=\"new\">\n        <Sidebar />\n        <div className=\"newContainer\">\n          <Navbar />\n          <div className=\"top\">\n            <h1>{title}</h1>\n          </div>\n          {bookMap ? bookMap.map((item, key) =>\n          <div className=\"bottom\" key={item.id}>\n            <div className=\"left\">\n              <img\n                  src={\n                    file\n                        ? URL.createObjectURL(file)\n                        : item.img\n                  }\n                  alt=\"\"\n              />\n            </div>\n            <div className=\"right\">\n              <form onSubmit={handleUpdate}>\n                <div className=\"formInput\">\n                  <label htmlFor=\"file\">\n                    Image: <DriveFolderUploadOutlinedIcon className=\"icon\" />\n                  </label>\n                  <input\n                      type=\"file\"\n                      id=\"file\"\n                      onChange={(e) => setFile(e.target.files[0])}\n                      style={{ display: \"none\" }}\n                  />\n                </div>\n                    <div className=\"formInput\">\n                      <label>ID</label>\n                      <input type=\"text\" value={valId ? valId : item.id} placeholder={item.id} onChange={handleChangeId}/>\n\n                      <label>TITLE</label>\n                      <input type=\"text\" value={bookname ?  bookname : \"\" } placeholder={ item.bookname } onChange={handleChangebookname} />\n\n                      <label>AUTHOR</label>\n                      <input type=\"text\" value={author ? author : \"\"} placeholder={item.author} onChange={handleChangeauthor}/>\n\n                      <label>STOCK</label>\n                      <input type=\"text\"  value={stock ? stock : \"\"} placeholder={item.stock} onChange={handleChangestock}/>\n\n                      <label>GENRE</label>\n                      <input type=\"text\"  value={genre ? genre : \"\"} placeholder={item.genre} onChange={handleChangegenre}/>\n                    </div>\n              </form>\n              <button onClick={handleCancel}>CANCEL</button>\n              <button onClick={handleUpdate}>UPDATE</button>\n            </div>\n          </div>\n          ) :\n              (\n                  <div className=\"new\">\n                    <Sidebar />\n                    <div className=\"newContainer\">\n                      <Navbar />\n                      <div className=\"top\">\n                        <h1>{title}</h1>\n                      </div>\n                      <div className=\"bottom\">\n                        <div className=\"left\">\n                          <img\n                              src={\n                                file\n                                    ? URL.createObjectURL(file)\n                                    : \"https://icon-library.com/images/no-image-icon/no-image-icon-0.jpg\"\n                              }\n                              alt=\"\"\n                          />\n                        </div>\n                        <div className=\"right\">\n                          <form>\n                            <div className=\"formInput\">\n                              <label htmlFor=\"file\">\n                                Image: <DriveFolderUploadOutlinedIcon className=\"icon\" />\n                              </label>\n                              <input\n                                  type=\"file\"\n                                  id=\"file\"\n                                  onChange={(e) => setFile(e.target.files[0])}\n                                  style={{ display: \"none\" }}\n                              />\n                            </div>\n\n                            {inputs.map((input) => (\n                                <div className=\"formInput\" key={input.id}>\n                                  <label>{input.label}</label>\n                                  <input type={input.type} placeholder={input.placeholder} />\n                                </div>\n\n                            ))}\n\n                          </form>\n                          <button>Send</button>\n                        </div>\n                      </div>\n\n                    </div>\n                  </div>\n              )\n\n          }\n        </div>\n      </div>\n  );\n};\n\nexport default Edit;\n\n\n",[],"/Users/mandlenkosibernard/Documents/GitHub/Team27/bookworm_admin/src/pages/new/New.jsx",[],[],{"ruleId":"156","severity":1,"message":"157","line":20,"column":18,"nodeType":"158","messageId":"159","endLine":20,"endColumn":25},{"ruleId":"160","severity":1,"message":"161","line":28,"column":32,"nodeType":"162","endLine":28,"endColumn":34,"suggestions":"163"},{"ruleId":"156","severity":1,"message":"164","line":4,"column":15,"nodeType":"158","messageId":"159","endLine":4,"endColumn":26},{"ruleId":"156","severity":1,"message":"165","line":4,"column":28,"nodeType":"158","messageId":"159","endLine":4,"endColumn":37},{"ruleId":"156","severity":1,"message":"166","line":13,"column":9,"nodeType":"158","messageId":"159","endLine":13,"endColumn":17},{"ruleId":"156","severity":1,"message":"157","line":23,"column":16,"nodeType":"158","messageId":"159","endLine":23,"endColumn":23},{"ruleId":"160","severity":1,"message":"167","line":35,"column":30,"